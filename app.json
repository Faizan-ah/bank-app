{
  "expo": {
    "name": "bank-app",
    "slug": "bank-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bank-app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.faizan.ah.bankapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/loading-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.faizan.ah.bankapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/loading-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/loading-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b69f2249-222a-4ce3-ad81-f33775e2ff4e"
      }
    }
  }
}
